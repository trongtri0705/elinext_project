<script>
    $('a.delete').click(function (e) {
        e.preventDefault();

        bootbox.confirm("Are you sure?", function (result) {
            if (result) {
                //
                 // $(this).closest('form').submit();
                 document.location.href = (e.target.href);
             }else return;
         });
    });            
    $("div.alert").delay(15000).slideUp();

</script>
